{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, {
            "singleline": "never",
            "multiline": {
              "objects": "always",
              "arrays": "always",
              "functions": "never",
              "typeLiterals": "ignore"
            }
          }],
          "quotes": [2, "single", { "avoidEscape": true }],
          "quotemark": [true, "single", "avoid-escape"],
          "object-literal-sort-keys": false,
          "ordered-imports": false
    },
    "rulesDirectory": []
}